{
  "name": "VOCALOID相互リンク",
  "version": "1.10",
  "manifest_version": 3,
  "description": "ボカロ曲のYouTube版とニコニコ動画版をワンクリックで行き来できます",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "action": { 
    "default_title": "リンク先に移動します"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ]
}